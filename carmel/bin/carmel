#!/bin/bash
if [ -z "$ARCH" ] ; then
u=`uname`
ARCH=cygwin
if [ $u = Linux ] ; then
 ARCH=linux
fi
if [ $u = SunOS ] ; then
 ARCH=solaris
fi
if [ $u = Darwin ] ; then
 ARCH=macosx
fi
fi
realprog=$0
if [ -L $realprog ] ; then
 if [ -x "`which readlink`" ] ; then
    realprog=`readlink $0`
 fi
fi
d=`dirname $realprog`

exec $d/$ARCH/carmel.static

