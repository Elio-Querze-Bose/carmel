[user]
  name = graehl
  email = graehl@gmail.com
  logallrefupdates = true
[github]
  user = graehl
  token = c16024811fee18c08ac842ba3d4547c5
[svn]
  authorsfile = /Users/graehl/t/graehl/util/.svn.authorsfile
[color]
  ui = on
[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold
[color "status"]
    added = yellow
    changed = green
    untracked = cyan

[alias]
    what = whatchanged
    st = status
    ci = commit
    br = branch
    co = checkout
    df = diff
    dc = diff --cached
    lg = log -p
    lol = log --graph --decorate --pretty=oneline --abbrev-commit
    lola = log --graph --decorate --pretty=oneline --abbrev-commit --all
    ls = ls-files
    info = git rev-parse HEAD
    # Show files ignored by git:
    ign = ls-files -o -i --exclude-standard
[alias]
    lg = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative
  lcrev = log --reverse --no-merges --stat @{1}..
[push]
  default = matching
[core]
  ignorecase = false
  excludesfile = /Users/graehl/.gitignore_global
  trustctime = false
	autocrlf = input
	whitespace = cr-at-eol
[diff]
  algorithm = patience
  renameLimit = 500
[merge]
  conflictstyle = diff3
  rename-threshold = 80
  ignore-space-change = 1
	renamelimit = 4000
[help]
  autocorrect = 10
[gc]
  reflogexpire = 300
[apply]
  whitespace = nowarn
[rerere]
  enabled = true
